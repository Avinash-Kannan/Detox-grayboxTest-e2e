{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios": {
      "type": "ios.app",
      "binaryPath": "ios/Build/Products/Debug-iphonesimulator/VodqaDetoxDemo.app",
      "build": "xcodebuild -workspace ios/VodqaDetoxDemo.xcworkspace -configuration Debug -scheme VodqaDetoxDemo -sdk iphonesimulator -derivedDataPath ios/build"
      
    },
    "android": {
      "type": "android.apk",
      "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
      "build": "cd android && ./gradlew assembleDebug assembleDebugAndroidTest -DtestBuildType=debug && cd /..",
      "buildios": "cd ios && xcodebuild -workspace VodqaDetoxDemo.xcworkspace -configuration Debug -scheme VodqaDetoxDemo"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 13"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "TestEmulator"
      }
    }
  },
  "configurations": {
    "ios": {
      "device": "simulator",
      "app": "ios"
    },
    "android": {
      "device": "emulator",
      "app": "android"
    }
  }
}